<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.swallowsnest.monrovia">

    <application
        android:allowBackup="true"
        android:icon="@drawable/monrovia_seal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArtActivity"
            android:label="Art in Monrovia"/>
        <activity android:name=".FilmingActivity"
            android:label="Filming Locations"/>
        <activity android:name=".RestaurantsActivity"
            android:label="Restaurants"/>
        <activity android:name=".ThingsActivity"
            android:label="Things To Do"/>

    </application>

</manifest>